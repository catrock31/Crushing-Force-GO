; Definition file for Jumbo Elephant
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Jumbo Elephant"       ;2nd Version       ;Name of character
displayname = "Jumbo"              ;Name of character to display
;startdate = 01,28,2020   	     ;Version date of character (MM-DD-YYYY or X.XX)
versiondate = 02,23,2018
mugenversion = 1.0                   ;Version of M.U.G.E.N character works on (X.XX)
author = "Kazecat"         	     ;Character author name
pal.defaults = 2,1,3,8,6,7,5                ;Default palettes in order of preference (up to 4)
                                     ;Numbering starts from 1
localcoord = 1280,720                 ;Local coordinate space width and height

; Files for the player
[Files]
cmd     = Jumbo.cmd           ;Command set
cns     = Jumbo.cns           ;Constants
cns     = common1.cns
st      = Jumbo.cns           ;States
stcommon = common1.cns      ;Common states (from data/ or motif)
sprite  = Jumbo.sff           ;Sprite
anim    = Jumbo.air           ;Animation
sound   = Jumbo.snd           ;Sound
ai      = Jumbo.ai            ;AI hints data (not used)

;Pal1 = Jumbo.act
pal1 = Base Pink.act
pal2 = Yokocosplay.act
Pal4 = Fishnets.act
pal7 = blacktights.act


;===========================
; Palette List
;===========================

; 1 - Pink Default
; 2 - Red
; 3 - White
; 4 - Skin
; 5 - Green
; 6 - Blue
; 7 - Black
; 8 - Yellow

; Maps character selection buttons to palette numbers.
[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6
x2 = 7 ;Hold Start and press button X for palette 7, etc.
y2 = 8
z2 = 9
a2 = 10
b2 = 11
c2 = 12


; Arcade mode
[Arcade]
intro.storyboard = 
ending.storyboard = 
